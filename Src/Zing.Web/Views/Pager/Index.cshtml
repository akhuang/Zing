@using Zing.Modules.Users.Models;
@model IPagedList<UserEntity>
@{
    ViewBag.Title = "Index";
}
<h2>
    Index</h2>
<table class="table">
    <thead>
        <tr>
            <th>
                Name
            </th>
            <th>
                Name
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.NormalizedUserName
                </td>
                <td>
                    @item.NormalizedUserName
                </td>
            </tr>
        }
    </tbody>
</table>
<div class="pager">
    @Html.Pager(Model.PageSize, Model.PageNumber, Model.TotalItemCount)
</div>
